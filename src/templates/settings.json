{
  "statusLine": {
    "type": "command",
    "command": "${DEVFLOW_DIR}/scripts/statusline.sh"
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "${DEVFLOW_DIR}/scripts/hooks/stop-update-memory.sh",
            "timeout": 10
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "${DEVFLOW_DIR}/scripts/hooks/session-start-memory.sh",
            "timeout": 10
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "${DEVFLOW_DIR}/scripts/hooks/pre-compact-memory.sh",
            "timeout": 10
          }
        ]
      }
    ]
  },
  "teammateMode": "auto",
  "env": {
    "ENABLE_TOOL_SEARCH": "true",
    "ENABLE_LSP_TOOL": "true",
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "extraKnownMarketplaces": {
    "devflow": {
      "source": {
        "source": "github",
        "repo": "dean0x/devflow"
      }
    }
  },
  "attribution": {
    "commit": "",
    "pr": ""
  }
}
